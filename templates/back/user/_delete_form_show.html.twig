<form method="post" class ="col-1" action="{{ path('app_back_user_delete', {'id': user.id}) }}" onsubmit="return confirm('Etes vous sur de vouloir supprimer «{{user.nickname}}» ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
        <button class="btn btn-danger col-12 mt-1">Supprimer</button>
</form>
